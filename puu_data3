#	(select	pa11.PRD_IRL_NBR  PRD_IRL_NBR,
		max(pa11.PRD_DESC)  PRD_DESC,
		max(pa11.DIV_NBR)  DIV_NBR,
		max(pa11.ITM_NBR0)  ITM_NBR0,
