	from	(select	a11.WK_NBR  WK_NBR,
			a12.PRD_IRL_NBR  PRD_IRL_NBR,
			max(a12.PRD_DESC)  PRD_DESC,
			max(a12.DIV_NBR)  DIV_NBR,
			max(a12.ITM_NBR)  ITM_NBR0,
